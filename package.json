    "babel-loader": "^8.0.5",
    "expose-loader": "^0.7.5",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "babel-jest": "^24.1.0",
    "jest-cli": "^24.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1"
